{
    "name": "job1",
    "host": "https://n.netease.com",
    "source": [
        {
            "type": "expr",
            "value": "https://n.netease.com/forum-74-{range}.html",
            "param": [
                {
                    "key": "range",
                    "value": "range(1,3)"
                }
            ]
        },
        {
            "type": "url",
            "value": "https://n.netease.com/forum-74-3.html"
        }
    ],
    "match": [
        {
            "type": "xpath",
            "value": "//table[@id='threadlisttableid']"
        }
    ],
    "filter": [],
    "target": [
        {
            "name": "subjob1",
            "match": [
                {
                    "type": "xpath",
                    "value": ".//tbody/tr"
                }
            ],
            "filter": [
                {
                    "type": "expr",
                    "value": "int('{p1}') > 20",
                    "param": [
                        {
                            "key": "p1",
                            "type": "xpath",
                            "value": ".//td[@class='num']/a[@class='xi2']/text()"
                        }
                    ]
                }
            ],
            "target": [
                {
                    "name": "href_page",
                    "match": [
                        {
                            "type": "xpath",
                            "value": ".//td[@class='icn']/a/@href"
                        }
                    ],
                    "target": [
                        {
                            "name": "inner_content",
                            "source": [{
                                "type": "expr",
                                "value": "'{host}'+'/{route}'",
                                "param": [
                                    {
                                        "key": "host",
                                        "type": "python",
                                        "value": "this.host"
                                    },
                                    {
                                        "key": "route",
                                        "type": "python",
                                        "value": "this.source"
                                    }
                                ]
                            }],
                            "match": [
                                {
                                    "type": "xpath",
                                    "value": "(//td[@class='t_f'])[1]/text()"
                                }
                            ]
                        }
                    ]
                },
                {
                    "name": "title",
                    "match": [
                        {
                            "type": "xpath",
                            "value": ".//a[@class='s xst']/text()"
                        }
                    ],
                    "store": {
                        "field": "title"
                    }
                }
            ]
        }
    ]
}